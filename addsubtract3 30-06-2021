import java util.*;
class Solution{
public static int SubtractProductAddSum(int n){
int sum=0,rem=0,mul=1;
while(n>0){
rem=n%10;
sum=sum+rem;
n/=10;
}
return mul-sum
}
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
System.out.println(SubtractProductAddSum(n));
}
}
----------------------------------------------------------------------------------------
OUTPUT:
Accepted.
Your input : 234
Output     : 15
Expected   : 15
